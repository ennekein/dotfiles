# vi: ft=sh

#####
# Environment
export EDITOR=`which vim`
export USE_EDITOR=$EDITOR
export VISUAL=$EDITOR

# chriskempson/base16-shell
export BASE16_SHELL_HOOKS=$HOME/.config/base16-shell/hooks

# nojhan/liquidprompt
export LP_ENABLE_TIME=1

# Python options
if [ -e ~/.pythonrc ]; then 
    export PYTHONSTARTUP=~/.pythonrc
    export PYTHONDONTWRITEBYTECODE=1 # don't write .pyc files to disk
    if [ -e /usr/local/bin/virtualenvwrapper.sh ]; then 
        export VIRTUALENVWRAPPER_PYTHON='/usr/bin/python3'
        export WORKON_HOME=$HOME/.virtualenvs
        source /usr/local/bin/virtualenvwrapper.sh
    fi
fi

# set PATH so it includes user's private bin if it exists
[ -d "$HOME/bin" ] && export PATH=$HOME/bin:/usr/local/bin:$PATH

# # colorize less
command -v pygmentize >/dev/null 2>&1 && export LESSOPEN='|pygmentize %s' 

